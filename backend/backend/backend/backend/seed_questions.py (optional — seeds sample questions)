from database import create_tables
from utils import add_question
create_tables()
sample = [
    ('What is 2+2?', '3','4','5','6','4'),
    ('Capital of France?', 'London','Berlin','Paris','Rome','Paris'),
    ('Which language runs in browsers?', 'Python','JavaScript','C++','Java','JavaScript')
]
for q in sample:
    add_question(*q)
print('Seeded sample questions.')
